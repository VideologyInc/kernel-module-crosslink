{
    "env": {
        "kernel_artifacts": "${workspaceFolder}/../../../tmp/work-shared/scailx-imx8mp/kernel-build-artifacts/",
        "kernel_path": "${workspaceFolder}/../../../tmp/work-shared/scailx-imx8mp/kernel-build-artifacts/"
    },
    "configurations": [
        {
            "name": "kernel-module-devtool",
            "includePath": [
                "${kernel_artifacts}/arch/arm64/include/**",
                "${kernel_artifacts}/arch/arm64/include/generated/**",
                "${kernel_path}/include/**",
                "${kernel_path}/arch/arm64/include/**",
                "${workspaceFolder}/oe-workdir/recipe-sysroot/usr/include/**"
            ],
            "defines": [ "_LINUX", "__KERNEL__" ],
            "forcedInclude": ["${kernel_artifacts}/include/generated/autoconf.h"],
            "compilerPath": "${workspaceFolder}/oe-workdir/recipe-sysroot-native/usr/bin/aarch64-poky-linux/aarch64-poky-linux-g++",
            "cppStandard": "c++11",
            "intelliSenseMode": "linux-gcc-arm64"
        }
    ],
    "version": 4
}